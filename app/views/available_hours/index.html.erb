<p id="notice"><%= notice %></p>

<h1>Available Hours</h1>

<table>
  <thead>
    <tr>
      <th>From</th>
      <th>To</th>
      <th>Tutor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @available_hours.each do |available_hour| %>
      <tr>
        <td><%= available_hour.from %></td>
        <td><%= available_hour.to %></td>
        <td><%= available_hour.tutor_id %></td>
        <td><%= link_to 'Show', available_hour %></td>
        <td><%= link_to 'Edit', edit_available_hour_path(available_hour) %></td>
        <td><%= link_to 'Destroy', available_hour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Available Hour', new_available_hour_path %>
